dhtmlXCalendarObject.prototype.langData["ch"] = {
    monthesFNames: ["一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月"],
    monthesSNames: ["一月","二月","三月","四月","五月","六月","七月","八月","九月","十月","十一月","十二月"],
    daysFNames: ["星期日","星期一","星期二","星期三","星期四","星期五","星期六"],
    daysSNames: ["日","一","二","三","四","五","六"],
    weekstart: 1
}
$(document).ready(function()
{
	//datepicker
	var myCalendar;
	var DhtmlxDate = new Array();
	DhtmlxDate = $(".dhtmlxcalendar").each( function(i){$(".dhtmlxcalendar").attr("id");});
    myCalendar = new dhtmlXCalendarObject(DhtmlxDate);
	myCalendar.enableTime=true;
	myCalendar.setDateFormat("%Y-%m-%d");
	myCalendar.loadUserLanguage("ch");

});

