		this.flag = flag;
	}

	public String toString() {
		return new ToStringBuilder(this).append("PkId", getPkId()).append("OldCode", getOldCode()).append("LoCode", getLoCode()).append("LoId", getLoId())
				.append("LoProvinceCode", getLoProvinceCode()).append("LoParties", getLoParties())
				.append("LoNum", getLoNum()).append("LoName", getLoName()).append("LoContact", getLoContact())
				.append("LoTel", getLoTel()).append("LoAddress", getLoAddress()).append("LoBank", getLoBank())
				.append("LoBankadd", getLoBankadd()).append("LoBankaccount", getLoBankaccount())
				.append("LoTaxpayerCodeentification", getLoTaxpayerCodeentification())
				.append("LoCreditcode", getLoCreditcode()).append("LoRepresentative", getLoRepresentative())
				.append("Remarks", getRemarks()).append("Flag", getFlag())
				.append("basicLinkinvestor", getBasicLinkinvestor()).append("basicTollroad", getBasicTollroad())
				.toString();
	}

	public int hashCode() {
		return new HashCodeBuilder().append(getPkId()).append(getOldCode()).append(getLoCode()).append(getLoId()).append(getLoProvinceCode())
				.append(getLoParties()).append(getLoNum()).append(getLoName()).append(getLoContact()).append(getLoTel())
				.append(getLoAddress()).append(getLoBank()).append(getLoBankadd()).append(getLoBankaccount())
				.append(getLoTaxpayerCodeentification()).append(getLoCreditcode()).append(getLoRepresentative())
				.append(getRemarks()).append(getFlag()).append(getBasicLinkinvestor()).append(getBasicTollroad()).toHashCode();
	}

	public boolean equals(Object obj) {
		if (obj instanceof BasicLinkowner == false)
			return false;
		if (this == obj)
			return true;
		BasicLinkowner other = (BasicLinkowner) obj;
		return new EqualsBuilder().append(getPkId(), other.getPkId()).append(getOldCode(), other.getOldCode())
				.append(getLoCode(), other.getLoCode()).append(getLoId(), other.getLoId()).append(getLoProvinceCode(), other.getLoProvinceCode())
				.append(getLoParties(), other.getLoParties()).append(getLoNum(), other.getLoNum())
				.append(getLoName(), other.getLoName()).append(getLoContact(), other.getLoContact())
				.append(getLoTel(), other.getLoTel()).append(getLoAddress(), other.getLoAddress())
				.append(getLoBank(), other.getLoBank()).append(getLoBankadd(), other.getLoBankadd())
				.append(getLoBankaccount(), other.getLoBankaccount())
				.append(getLoTaxpayerCodeentification(), other.getLoTaxpayerCodeentification())
				.append(getLoCreditcode(), other.getLoCreditcode())
				.append(getLoRepresentative(), other.getLoRepresentative())
				.append(getBasicLinkinvestor(), other.getBasicLinkinvestor())
				.append(getRemarks(), other.getRemarks())
				.append(getFlag(), other.getFlag())
				.append(getBasicTollroad(), other.getBasicTollroad()).isEquals();
	}
